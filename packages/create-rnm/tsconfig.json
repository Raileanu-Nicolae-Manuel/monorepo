{
  "include": ["src"],
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2023",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "types": ["node"],
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "noEmit": false
  }
}